{
	"version": "2.0.0",
	"name": "mockbin",
	"description": "Test, mock, and track HTTP requests & responses between libraries, sockets and APIs",
	"author": "Kong (https://www.konghq.com/)",
	"homepage": "https://mockbin.org/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Kong/mockbin.git"
	},
	"license": "MIT",
	"main": "lib/index",
	"bin": "bin/mockbin",
	"keywords": [
		"api",
		"bin",
		"har",
		"http",
		"json",
		"mock",
		"mocking",
		"test",
		"testing"
	],
	"engines": {
		"node": ">=22"
	},
	"files": ["bin", "docs", "src", "lib", "server.js"],
	"bugs": {
		"url": "https://github.com/Kong/mockbin/issues"
	},
	"scripts": {
		"start": "node server.js",
		"dev": "npx nodemon -e pug,js,json server.js",
		"test": "mocha --recursive",
		"check": "biome check .",
		"coverage": "npx nyc --report-dir coverage npm test",
		"clean": "git clean -fdX"
	},
	"devDependencies": {
		"@biomejs/biome": "1.6.4",
		"mocha": "^10.2.0",
		"should": "^13.2.3"
	},
	"dependencies": {
		"change-case": "^4.1.1",
		"compression": "^1.7.4",
		"content-type": "^1.0.4",
		"cookie-parser": "^1.4.5",
		"debug": "^4.3.4",
		"dicer": "Kong/dicer",
		"dotenv": "^16.4.0",
		"express": "^4.17.1",
		"har-validator": "^5.1.3",
		"jstransformer-marked": "^1.4.0",
		"method-override": "^3.0.0",
		"morgan": "^1.9.1",
		"pug": "^3.0.3",
		"redis": "^4.6.12",
		"require-directory": "^2.1.1",
		"uuid": "^8.3.2",
		"xmlbuilder": "^15.0.0",
		"yamljs": "^0.3.0"
	}
}
